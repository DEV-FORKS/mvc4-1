@model eManager.Domain.Department

@{
    ViewBag.Title = "Detail";
}

<h2>@Model.Name</h2>



@Html.ActionLink("Create an Employee", "Create", "Employee", new { departmentId = Model.Id }, null)

<table border="0" cellpadding="0" cellspacing="0">
    <tr>
        <th>Name</th>
        <th></th>
    </tr>
    @foreach (var item in Model.Employees)
    {
        <tr>
        <td> @item.Name </td>
        <td> </td>
        </tr>
    }
</table>
